#% if agentgateway_addr is defined %#
---
#| Service to expose AgentGateway UI on port 15000 #|
apiVersion: v1
kind: Service
metadata:
  name: agentgateway-ui
  labels:
    app.kubernetes.io/name: agentgateway-ui
    app.kubernetes.io/component: ui
    app.kubernetes.io/part-of: kgateway
spec:
  selector:
    app.kubernetes.io/name: agentgateway
  ports:
    - name: http
      port: 15000
      targetPort: 15000
      protocol: TCP
#% endif %#
