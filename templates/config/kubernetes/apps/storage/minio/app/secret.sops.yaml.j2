#% if minio_enabled | default(false) %#
---
# MinIO root credentials (admin access)
# Used by HelmRelease via existingSecret
apiVersion: v1
kind: Secret
metadata:
  name: minio-credentials
type: Opaque
stringData:
  rootUser: "#{ minio_root_user | default('admin') }#"
  rootPassword: "#{ minio_root_password }#"
#% endif %#
