#% if minio_enabled | default(false) or cognee_enabled | default(false) %#
---
apiVersion: kustomize.config.k8s.io/v1beta1
kind: Kustomization
namespace: storage

components:
  - ../../components/sops

resources:
  - ./namespace.yaml
#% if minio_enabled | default(false) %#
  - ./minio/ks.yaml
#% endif %#
#% if cognee_enabled | default(false) %#
  - ./neo4j/ks.yaml
#% endif %#
#% endif %#
