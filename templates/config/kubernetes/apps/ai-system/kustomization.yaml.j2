#% if envoy_ai_gateway_enabled and (azure_openai_us_east_api_key or azure_openai_us_east2_api_key) %#
---
apiVersion: kustomize.config.k8s.io/v1beta1
kind: Kustomization
namespace: ai-system

components:
  - ../../components/sops

resources:
  - ./namespace.yaml
#% if azure_openai_us_east_api_key %#
  - ./azure-openai-us-east/ks.yaml
#% endif %#
#% if azure_openai_us_east2_api_key %#
  - ./azure-openai-us-east2/ks.yaml
  - ./azure-anthropic/ks.yaml
#% endif %#
#% endif %#
