#% if agentgateway_enabled | default(true) %#
---
apiVersion: kustomize.toolkit.fluxcd.io/v1
kind: Kustomization
metadata:
  name: agentgateway-crds
spec:
  interval: 1h
  path: ./kubernetes/apps/ai-system/agentgateway-crds/app
  prune: true
  sourceRef:
    kind: GitRepository
    name: flux-system
    namespace: flux-system
  targetNamespace: ai-system
  wait: true
#% endif %#
