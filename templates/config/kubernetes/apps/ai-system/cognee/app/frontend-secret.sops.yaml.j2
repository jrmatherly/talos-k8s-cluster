#% if cognee_enabled | default(false) and cognee_frontend_enabled | default(false) %#
---
apiVersion: v1
kind: Secret
metadata:
  name: cognee-frontend-secrets
type: Opaque
stringData:
  # Auth0 Configuration (optional - leave empty to disable authentication)
  AUTH0_DOMAIN: "#{ cognee_auth0_domain | default('') }#"
  AUTH0_CLIENT_ID: "#{ cognee_auth0_client_id | default('') }#"
  AUTH0_CLIENT_SECRET: "#{ cognee_auth0_client_secret | default('') }#"
  AUTH0_SECRET: "#{ cognee_auth0_secret | default('') }#"
#% endif %#
