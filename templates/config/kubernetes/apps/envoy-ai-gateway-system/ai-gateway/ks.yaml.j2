#% if envoy_ai_gateway_enabled %#
---
apiVersion: kustomize.toolkit.fluxcd.io/v1
kind: Kustomization
metadata:
  name: ai-gateway
spec:
  interval: 1h
  path: ./kubernetes/apps/envoy-ai-gateway-system/ai-gateway/app
  dependsOn:
    - name: ai-gateway-crds
  prune: true
  sourceRef:
    kind: GitRepository
    name: flux-system
    namespace: flux-system
  targetNamespace: envoy-ai-gateway-system
  wait: true
#% endif %#
