#% if ai_gateway_enabled %#
---
apiVersion: kustomize.toolkit.fluxcd.io/v1
kind: Kustomization
metadata:
  name: ai-gateway-controller
spec:
  interval: 1h
  path: ./kubernetes/apps/ai-gateway/controller/app
  dependsOn:
    - name: ai-gateway-crds
  prune: true
  sourceRef:
    kind: GitRepository
    name: flux-system
    namespace: flux-system
  targetNamespace: #{ ai_gateway_namespace }#
  wait: true
#% endif %#
