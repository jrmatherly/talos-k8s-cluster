#% if ai_gateway_enabled %#
---
apiVersion: source.toolkit.fluxcd.io/v1
kind: OCIRepository
metadata:
  name: ai-gateway-crds
spec:
  interval: 15m
  layerSelector:
    mediaType: application/vnd.cncf.helm.chart.content.v1.tar+gzip
    operation: copy
  ref:
    tag: #{ ai_gateway_version }#
  url: oci://docker.io/envoyproxy/ai-gateway-crds-helm
#% endif %#
